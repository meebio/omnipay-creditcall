        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "creditcall",
        "cardeasexml"
    ],
    "homepage": "https://github.com/meebio/omnipay-creditcall",
    "license": "MIT",
    "authors": [
        {
            "name": "John Jablonski",
            "email": "jablonski.kce@gmail.com",
            "homepage": "http://jablonski.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "omnipay/common": "~2.0"
    },
    "require-dev": {
        "omnipay/tests": "~2.0",
        "scrutinizer/ocular": "~1.1"
